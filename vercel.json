{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install && npx prisma generate",
        "buildCommand": "npm run build",
        "includeFiles": [
          "src/**/*",
          "prisma/schema.prisma",
          "node_modules/.prisma/client/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
